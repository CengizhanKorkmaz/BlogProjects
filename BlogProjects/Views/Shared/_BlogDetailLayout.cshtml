
<!DOCTYPE html>
<html lang="zxx">
<head>
    @await Html.PartialAsync("LayoutPartials/_Head");
    <link rel="stylesheet" href="~/web/css/single.css">

</head>
<body>
<!--Header-->
@await Html.PartialAsync("LayoutPartials/_Header")
<!--//header-->
<div class="banner-inner">
</div>

<!--//banner-->
<section class="banner-bottom">
    <!--/blog-->
    <div class="container">
        <div class="row">
            <!--left-->
            <div class="col-lg-8 left-blog-info-w3layouts-agileits text-left">
                @RenderBody()

            </div>

            <!--//left-->
            <!--right-->
            @await Html.PartialAsync("LayoutPartials/_RightBody")
            <!--//right-->
        </div>
    </div>
</section>
<!--//main-->
<!--footer-->
@await Html.PartialAsync("LayoutPartials/_Footer")
<script src="https://code.jquery.com/jquery-3.5.1.min.js" type="text/javascript"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
<script src="~/web/js/move-top.js"></script>
<script src="~/web/js/easing.js"></script>
<script src="~/web/js/bootstrap.js"></script>

<script>
    jQuery(document).ready(function($) {
        $(".scroll").click(function(event) {
            event.preventDefault();
            $('html,body').animate({
                    scrollTop: $(this.hash).offset().top
                },
                900);
        });
    });
</script>
<!--// end-smoth-scrolling -->

<script>
    $(document).ready(function() {
        /*
                                var defaults = {
                                      containerID: 'toTop', // fading element id
                                    containerHoverID: 'toTopHover', // fading element hover id
                                    scrollSpeed: 1200,
                                    easingType: 'linear'
                                 };
                                */

        $().UItoTop({
            easingType: 'easeOutQuart'
        });

    });
</script>


</body>

</html>